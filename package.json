{
  "name": "integration_web",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run build:tailwind && npm run build:scss",
    "build:tailwind": "tailwindcss -i ./tailwind.css -o ./public/dist/output.css --minify",
    "build:scss": "sass src/styles/main.scss:public/style.css --style compressed --quiet",
    "dev": "concurrently \"npm run watch:tailwind\" \"npm run watch:scss\"",
    "watch:tailwind": "tailwindcss -i ./tailwind.css -o ./dist/output.css --watch",
    "watch:scss": "sass --watch src/styles/main.scss:public/style.css --quiet"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.38",
    "postcss-scss": "^4.0.9",
    "sass": "^1.77.0",
    "tailwindcss": "^3.4.19"
  }
}
